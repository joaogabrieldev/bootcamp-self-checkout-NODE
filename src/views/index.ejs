<!DOCTYPE html>
<html lang="pt-BR">

<%- include('./partials/head.ejs') %>
    <%- include('./partials/navbar.ejs') %>

        <body>
            <div class="content">

                <% if (users.length> 0) { %> <% users.forEach(user=> { %>
                        <h3 class="title">
                            <%= user.firstName %>
                                <%= user.lastName%>
                        </h3>

                        <p>
                            <%= user.email %>
                        </p>

                        <% })%>
                            <% } else {%>
                                <p>Não há usuarios para exibir...</p> %>
                                <% }%>

            </div>

        </body>

</html>